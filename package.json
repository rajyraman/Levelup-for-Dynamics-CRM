{
  "name": "level-up-vnext",
  "version": "4.1.2",
  "description": "Microsoft Edge sidebar extension for Dynamics 365/Power Apps productivity tools",
  "main": "build/background.js",
  "type": "module",
  "imports": {
    "#types/*": "./src/types/*",
    "#services/*": "./src/services/*",
    "#components/*": "./src/sidebar/components/*",
    "#contexts/*": "./src/sidebar/contexts/*",
    "#hooks/*": "./src/sidebar/hooks/*",
    "#config/*": "./src/sidebar/config/*",
    "#utils/*": "./src/utils/*",
    "#content/*": "./src/content/*",
    "#background/*": "./src/background/*",
    "#data/*": "./src/data/*"
  },
  "scripts": {
    "build": "vite build --mode production",
    "dev": "cross-env NODE_ENV=development vite build --watch",
    "watch": "vite build --mode production --watch",
    "clean": "rimraf build release",
    "pack": "npm run clean && npm run build && node scripts/package.js && node scripts/package.js firefox",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verify": "node scripts/test-verification.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,css,html}\"",
    "code-quality": "npm run lint && npm run format:check && npm run type-check",
    "code-quality:fix": "npm run lint:fix && npm run format",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run code-quality:fix && npm run test && npm run build",
    "validate": "npm run code-quality && npm run test && npm run build",
    "size": "npm run build && node -e \"const fs=require('fs'); const files=['sidebar.js','levelup-extension.js','background.js']; files.forEach(f => { try { const stats = fs.statSync('build/'+f); console.log(f + ': ' + (stats.size/1024).toFixed(2) + ' KB'); } catch(e) { console.log(f + ': not found'); } });\"",
    "bundle-analyze": "npx vite-bundle-analyzer build",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "bump": "node scripts/bump-version.js",
    "tag": "node -e \"const v=require('./package.json').version; require('child_process').execSync(`git tag v${v} && git push origin v${v}`, {stdio:'inherit'})\"",
    "tag:check": "node -p \"require('./package.json').version\"",
    "update-release-notes": "node scripts/update-release-notes.js"
  },
  "keywords": [
    "dynamics-365",
    "browser-extension",
    "sidebar",
    "productivity",
    "power-apps"
  ],
  "author": "Natraj Yegnaraman",
  "homepage": "https://github.com/rajyraman/Levelup-for-Dynamics-CRM",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.12",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/xrm": "^9.0.87",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "archiver": "^6.0.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-puppeteer": "^11.0.0",
    "msw": "^2.10.5",
    "prettier": "^3.2.5",
    "puppeteer": "^24.17.0",
    "puppeteer-core": "^24.17.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^6.0.7"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@playwright/test": "^1.54.2",
    "@uiw/react-codemirror": "^4.24.2",
    "dynamics-web-api": "^2.3.1",
    "otplib": "^12.0.1",
    "playwright": "^1.54.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}

