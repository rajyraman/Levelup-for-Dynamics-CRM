{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Extension",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"build"
			],
			"group": "build",
			"problemMatcher": [
				"$tsc"
			],
			"isBackground": false
		},
		{
			"label": "Watch Extension",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"group": "build",
			"problemMatcher": [
				"$tsc-watch"
			],
			"isBackground": true
		}
	]
}
